<Window x:Class="WpfViewsw.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfViewsw"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Image x:Name="Image6_png" Source="Image6.png" Stretch="Fill"/>
        <Button x:Name="btnCadastrar" Content="Cadastrar Clinte" HorizontalAlignment="Left" Margin="21,19,0,0" VerticalAlignment="Top" Width="127" Height="29" FontFamily="Arial Black" Click="btnCadastrar_Click"/>
        <Button x:Name="btnEntregador" Content="Cadastro Entregador" HorizontalAlignment="Left" Margin="21,53,0,0" VerticalAlignment="Top" Width="151" Height="29" FontFamily="Arial Black" Click="btnEntregador_Click"/>
        <Button x:Name="btnFornecedor" Content="Cadastro Fornecedor" HorizontalAlignment="Left" Margin="21,87,0,0" VerticalAlignment="Top" Width="151" Height="31" FontFamily="Arial Black" Click="btnFornecedor_Click"/>
    </Grid>
</Window>
